{
  "name": "@cognigy/webchat",
  "version": "2.37.0",
  "description": "Webchat Widget for Cognigy.AI",
  "author": "Cognigy GmbH <info@cognigy.com>",
  "contributors": [
    "Robin Schuster <r.schuster@cognigy.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "main": "lib/webchat/index.js",
  "scripts": {
    "build": "npm run bundle",
    "bundle": "npm run webchat && npm run webchat-legacy && npm run date-picker && npm run date-picker-legacy && npm run rating && npm run rating-legacy &&  npm run speech-input && npm run speech-input-legacy && npm run speech-output && npm run speech-output-legacy && npm run messenger && npm run messenger-legacy",
    "dev": "webpack-dev-server --config webpack.dev.js --content-base dist/ --host 0.0.0.0 --disable-host-check",
    "date-picker-legacy": "webpack --config webpack.production-plugin.legacy.js --entry ./src/plugins/date-picker/index.tsx --output dist/date-picker.webchat-plugin.legacy.js",
    "date-picker": "webpack --config webpack.production-plugin.js --entry ./src/plugins/date-picker/index.tsx --output dist/date-picker.webchat-plugin.js",
    "messenger": "webpack --config webpack.production-plugin.js --entry ./src/plugins/messenger/index.tsx --output dist/messenger.webchat-plugin.js",
    "messenger-legacy": "webpack --config webpack.production-plugin.legacy.js --entry ./src/plugins/messenger/index.tsx --output dist/messenger.webchat-plugin.legacy.js",
    "rating-legacy": "webpack --config webpack.production-plugin.legacy.js --entry ./src/plugins/rating/index.ts --output dist/rating.webchat-plugin.legacy.js",
    "rating": "webpack --config webpack.production-plugin.js --entry ./src/plugins/rating/index.ts --output dist/rating.webchat-plugin.js",
    "speech-input-legacy": "webpack --config webpack.production-plugin.legacy.js --entry ./src/plugins/speech-input/index.tsx --output dist/speech-input.webchat-plugin.legacy.js",
    "speech-input": "webpack --config webpack.production-plugin.js --entry ./src/plugins/speech-input/index.tsx --output dist/speech-input.webchat-plugin.js",
    "speech-output-legacy": "webpack --config webpack.production-plugin.legacy.js --entry ./src/plugins/speech-output/index.tsx --output dist/speech-output.webchat-plugin.legacy.js",
    "speech-output": "webpack --config webpack.production-plugin.js --entry ./src/plugins/speech-output/index.tsx --output dist/speech-output.webchat-plugin.js",
    "webchat-legacy": "webpack --config webpack.production.legacy.js --output dist/webchat.legacy.js",
    "webchat": "webpack --config webpack.production.js",
    "http-server": "http-server -a localhost -p 8888 dist/",
    "cypress:open": "cypress open",
    "cypress:serve": "npm run http-server",
    "cypress:run": "cypress run",
    "test": "run-p -r cypress:serve cypress:run",
    "pretest": "npm run webchat-legacy && npm run date-picker-legacy",
    "selenium-test": "node selenium-test.js",
    "browserstack-test": "run-p -r http-server selenium-test",
    "prebrowserstack-test": "npm run pretest"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^5.0.2",
    "@cognigy/socket-client": "^4.5.4",
    "@emotion/cache": "^10.0.19",
    "@emotion/core": "^10.0.22",
    "@emotion/provider": "^0.11.2",
    "@emotion/styled": "^10.0.23",
    "dompurify": "^2.0.7",
    "emotion": "^10.0.23",
    "emotion-theming": "^10.0.19",
    "flatpickr": "^4.6.3",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-animate-height": "^2.0.17",
    "react-dom": "^16.12.0",
    "react-emotion": "^10.0.0",
    "react-player": "^1.14.2",
    "react-redux": "^6.0.0",
    "react-responsive-carousel": "^3.1.51",
    "redux": "^4.0.4",
    "styled-jss": "^2.2.3",
    "stylis-rtl": "^1.0.1",
    "tinycolor2": "^1.4.1",
    "uifx": "^2.0.7"
  },
  "devDependencies": {
    "@actions/core": "^1.5.0",
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/runtime": "^7.7.2",
    "@types/dompurify": "^2.0.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^6.0.12",
    "@types/tinycolor2": "^1.4.2",
    "@types/uuid": "^3.4.6",
    "@types/whatwg-fetch": "0.0.33",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.0",
    "cypress": "^5.5.0",
    "http-server": "^0.12.3",
    "idempotent-babel-polyfill": "^7.4.4",
    "npm-run-all": "^4.1.5",
    "react-svg-loader": "^3.0.3",
    "redux-devtools-extension": "^2.13.8",
    "selenium-webdriver": "^4.0.0-rc-1",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-react-loader": "^0.4.6",
    "ts-loader": "^5.4.5",
    "typesafe-actions": "^3.0.0",
    "typescript": "^3.7.2",
    "url-loader": "^4.1.1",
    "url-polyfill": "^1.1.7",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "whatwg-fetch": "^3.0.0"
  }
}
